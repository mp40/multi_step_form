{
    "env": {
        "es6": true
    },
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": ["error", "always"],
      "react/jsx-filename-extension": [1, { "extensions": [".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": true
        }
      ]
    }
  }